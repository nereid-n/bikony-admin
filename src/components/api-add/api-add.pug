mixin api-add()
  .row
    .col-12
      h2.admin-table-title group add
      .group-add
        -
          var inputs = [
            'API Name*',
            'API Url*',
            'API User*',
            'API Password*'
          ]
        each item in inputs
          span.add-title= item
          .input-block
            input.width100
        button.btn.btn-blue.btn-big.my-4 Send
